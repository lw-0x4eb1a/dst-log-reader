{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Log Reader",
  "version": "0.1.0",
  "identifier": "com.dst-log-reader.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "log-reader",
        "width": 400,
        "height": 600,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
